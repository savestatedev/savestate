{
  "version": 2,
  "outputDirectory": "site",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@5.5.28"
    }
  },
  "rewrites": [
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/blog", "destination": "/blog/index.html" },
    { "source": "/blog/your-ai-knows-everything", "destination": "/blog/your-ai-knows-everything.html" },
    { "source": "/blog/how-savestate-works", "destination": "/blog/how-savestate-works.html" }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
